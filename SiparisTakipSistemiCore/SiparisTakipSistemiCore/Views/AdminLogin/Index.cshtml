
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Admin Giriş</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="icon" type="image/png" href="images/icons/favicon.ico" />
    <link rel="stylesheet" type="text/css" href="~/NiluferLogin/vendor/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="~/NiluferLogin/fonts/font-awesome-4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" type="text/css" href="~/NiluferLogin/fonts/iconic/css/material-design-iconic-font.min.css">
    <link rel="stylesheet" type="text/css" href="~/NiluferLogin/vendor/animate/animate.css">
    <link rel="stylesheet" type="text/css" href="~/NiluferLogin/vendor/css-hamburgers/hamburgers.min.css">
    <link rel="stylesheet" type="text/css" href="~/NiluferLogin/vendor/animsition/css/animsition.min.css">
    <link rel="stylesheet" type="text/css" href="~/NiluferLogin/vendor/select2/select2.min.css">
    <link rel="stylesheet" type="text/css" href="~/NiluferLogin/vendor/daterangepicker/daterangepicker.css">
    <link rel="stylesheet" type="text/css" href="~/NiluferLogin/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/NiluferLogin/css/main.css">
</head>
<body>
    <div class="container-login100" style="background-image: url('/NiluferLogin/images/bg-01.jpg');">
        <div class="wrap-login100 p-l-55 p-r-55 p-t-80 p-b-30">
            @using (Html.BeginForm("Index", "AdminLogin", FormMethod.Post))
            {
                <span class="login100-form-title p-b-37">
                    Admin Giriş
                </span>
                <div class="wrap-input100 validate-input m-b-20" data-validate="Enter username or email">
                    <input class="input100" type="text" name="AdminAd" placeholder="Kullanıcı Adı">
                    <span class="focus-input100"></span>
                </div>
                <div class="wrap-input100 validate-input m-b-25" data-validate="Enter password">
                    <input class="input100" type="password" name="AdminParola" placeholder="Şifre">
                    <span class="focus-input100"></span>
                </div>
                <div class="container-login100-form-btn">
                    <button type="submit" class="login100-form-btn">
                        Giriş Yap
                    </button>
                </div>
            }
        </div>
    </div>
    <div id="dropDownSelect1"></div>
    <script src="~/NiluferLogin/vendor/jquery/jquery-3.2.1.min.js"></script>
    <script src="~/NiluferLogin/vendor/animsition/js/animsition.min.js"></script>
    <script src="~/NiluferLogin/vendor/bootstrap/js/popper.js"></script>
    <script src="~/NiluferLogin/vendor/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/NiluferLogin/vendor/select2/select2.min.js"></script>
    <script src="~/NiluferLogin/vendor/daterangepicker/moment.min.js"></script>
    <script src="~/NiluferLogin/vendor/daterangepicker/daterangepicker.js"></script>
    <script src="~/NiluferLogin/vendor/countdowntime/countdowntime.js"></script>
    <script src="~/NiluferLogin/js/main.js"></script>

</body>
</html>
